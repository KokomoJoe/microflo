timer(Timer) OUT -> IN toggle(ToggleBoolean)
toggle() OUT -> IN switch(BreakBeforeMake)
switch() OUT1 -> IN ia(InvertBoolean) OUT -> IN turnOff(DigitalWrite) OUT -> IN ib(InvertBoolean) OUT -> MONITOR1 switch()
switch() OUT2 -> IN ic(InvertBoolean) OUT -> IN turnOn(DigitalWrite) OUT -> IN id(InvertBoolean) OUT -> MONITOR2 switch()

# Config
'5000' -> INTERVAL timer()
board(ArduinoUno) 8 -> PIN turnOff()
board() 9 -> PIN turnOn()




